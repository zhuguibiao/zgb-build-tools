{
  "compilerOptions": {
    "esModuleInterop": true,
    "types": ["node"],
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["__test__/**/*"],
  "exclude": ["node_modules"]
}
